#include <iostream>
using namespace std;

template <class T>
class Calculator {
public:
    T add(T a, T b) { return a + b; }
    T sub(T a, T b) { return a - b; }
    T mul(T a, T b) { return a * b; }
    T divi(T a, T b) { return a / b; }
};

int main() {
    Calculator<int> c1;
    cout << c1.add(10, 5) << endl;
    cout << c1.mul(10, 5) << endl;
    return 0;
}
