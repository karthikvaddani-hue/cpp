#include <iostream>
using namespace std;

// Function template
template <typename T>
T getMax(T a, T b) {
    return (a > b) ? a : b;
}

int main() {
    cout << "Max of 10 and 20 is: " << getMax(10, 20) << endl;
    cout << "Max of 3.5 and 2.1 is: " << getMax(3.5, 2.1) << endl;
    cout << "Max of 'A' and 'Z' is: " << getMax('A', 'Z') << endl;
    return 0;
}
